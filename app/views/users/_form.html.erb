<%= form_for(@user, html: {multipart: true}) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>

  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>

  <%= f.label :image %>
  <%= f.cl_image_upload :image %>
  <br>

  <%= f.submit %>

<% end %>
